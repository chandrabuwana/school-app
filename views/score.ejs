<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Score</title>
  </head>
  <body>
    <form action="/subject/<%=dataStudentSubject[0].SubjectId%>/<%=dataStudentSubject[0].StudentId%>/score" method="post">
      <table border="2px">
        <thead>
          <th>Student Name</th>
          <th>Subject Name</th>
          <th>Score</th>
          <th>Action</th>
        </thead>
        <tbody>
          <td><%=dataStudentSubject[0].Student.fullname()%></td>
          <td><%=dataStudentSubject[0].Subject.subject_name%></td>

          <td><input type="text" name="score" placeholder="Score"></td>
          <td><input type="submit" value="submit"></td>
        </tbody>
    </form>
    </table>
  </body>
</html>
