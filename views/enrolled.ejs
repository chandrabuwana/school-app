<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <a href="/subject">Back to Subject</a>
    <h3><%=dataStudentSubject[0].Subject.subject_name%></h3>
    <table border="2px">
      <thead>
        <th>No</th>
        <th>Id Student</th>
        <th>Student Name</th>
        <th>Description</th>
        <th>Score</th>
      </thead>
      <% let i=1
        dataStudentSubject.forEach(data=>{%>
      <tbody>
        <td><%=i%></td>
        <td><%=data.Student.id%></td>
        <td><%=data.Student.fullname()%></td>
        <%if(data.score){%>

        <td><%=data.desc()%></td>
        <%}else{%>
          <td></td>
          <%}%>
        <% if(data.score){%>
          <td><%=data.score%></td>
        <% }else { %>
          <td><a href="/subject/<%=data.SubjectId%>/<%=data.StudentId%>/score">Score</a></td>
        <% } %>
      </tbody>
      <%i++})%>
    </table>
  </body>
</html>
