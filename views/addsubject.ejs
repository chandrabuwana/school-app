<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <a href="/student">Back To Student</a>
    <thead>
      <th>First-name</th>
      <th>Last-name</th>
      <th>Email</th>
    </thead>
    <tbody>
      <tr>
        <td>
            <form action="/student/addsubject/<%=dataStudent.id%>" method="post">
              <input type="text" name="first_name" value="<%=dataStudent.first_name%>" placeholder="First Name">
              <input type="text" name="last_name" value="<%=dataStudent.last_name%>" placeholder="Last Name">
              <input type="text" name="email" value="<%=dataStudent.email%>" placeholder="Email">
              <select name="subject_name">
                <%dataSubject.forEach(data=>{%>
                  <option value="<%=data.id%>"><%=data.subject_name%></option>
                <%})%>
                </select>
                <input type="submit" value="submit">
          </form>
          </td>
      </tr>
    </tbody>
  </body>
</html>
